{% extends 'base.html.twig' %}
{% block body %}
	{% for video in videos %}
        <h2>{{ video.title }}</h2>
        <p>{{ video.url }}</p>
		<a href="{{ path('update_video', {'id': video.url}) }}" >Modifier</a>
		<a href="{{ path('delete_video', {'id': video.url}) }}" >Supprimer</a>
    {% endfor %}
{% endblock %}